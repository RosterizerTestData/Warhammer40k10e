      "Stratagem§Tailored Toxins": {
        "keywords": {
          "Keywords": [
            "Drukhari",
            "Kabalite Cartel"
          ],
          "Phase": [
            "Shooting",
            "Fight"
          ],
          "Category": [
            "Epic Deed"
          ],
          "Turn": [
